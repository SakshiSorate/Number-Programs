package p8;

public class RevInRange {
	public static int revNumber(int a)
	{
		int rev=0;
		while(a>0)
		{
			int rem=a%10;
			rev=rev*10+rem;
			a=a/10;
		}
		return rev;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		RevInRange ref=new RevInRange();
		int start=100;
		int end=300;
		for(int i=start;start<=end;start++)
		{
			System.out.println(revNumber(start));
		}
	}
	

}
